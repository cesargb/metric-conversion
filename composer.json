{
    "name": "cesargb/metric-conversion",
    "description": "Calculators for metric conversions",
    "type": "library",
    "require": {
        "php": "^7.3",
        "eloquent/enumeration": "^6.0"
    },
    "homepage": "https://github.com/cesargb/metric-conversion",
    "license": "MIT",
    "authors": [
        {
            "name": "Cesar Garcia",
            "email": "cesargb@gmail.com",
            "role": "Developer"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Cesargb\\Metric\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cesargb\\Metric\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "stylefix": "vendor/bin/php-cs-fixer fix"
    }
}
